{
  "name": "@cortex/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./backends": {
      "import": "./dist/backends/index.js",
      "types": "./dist/backends/index.d.ts"
    },
    "./buffer": {
      "import": "./dist/buffer/index.js",
      "types": "./dist/buffer/index.d.ts"
    },
    "./channels": {
      "import": "./dist/channels/index.js",
      "types": "./dist/channels/index.d.ts"
    },
    "./tracing": {
      "import": "./dist/tracing/index.js",
      "types": "./dist/tracing/index.d.ts"
    },
    "./memory": {
      "import": "./dist/memory/index.js",
      "types": "./dist/memory/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser/index.js",
      "types": "./dist/browser/index.d.ts"
    },
    "./skills": {
      "import": "./dist/skills/index.js",
      "types": "./dist/skills/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf dist .tsbuildinfo"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@qdrant/js-client-rest": "^1.12.0",
    "chokidar": "^4.0.0",
    "uuid": "^11.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cortex/config": "workspace:*",
    "@opentelemetry/sdk-trace-node": "^2.5.1",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "vitest": "^3.0.0"
  }
